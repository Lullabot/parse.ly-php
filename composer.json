{
    "name": "lullabot/parse.ly-php",
    "description": "Parse.ly API client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Berry",
            "email": "andrew.berry@lullabot.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^6.5|^7.5",
        "symfony/serializer": "^4.4|^6.1",
        "symfony/property-access": "^4.4|^6.1",
        "symfony/property-info": "^4.4|^6.1",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "autoload": {
        "psr-4": {
          "Lullabot\\Parsely\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Lullabot\\Parsely\\Tests\\": "tests/src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v2.19|^3.13",
        "phpunit/phpunit": "^8.5.23|^9.5",
        "symfony/console": "^4.4|6.1",
        "namshi/cuzzle": "^2.0",
        "rtheunissen/guzzle-log-middleware": "^v0.4.2"
    }
}
