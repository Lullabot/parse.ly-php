{
    "name": "lullabot/parse.ly-php",
    "description": "Parse.ly API client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Berry",
            "email": "andrew.berry@lullabot.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/serializer": "^3.4|^4.4",
        "symfony/property-access": "^3.4|^4.4",
        "symfony/property-info": "^3.4|^4.4"
    },
    "autoload": {
        "psr-4": {
          "Lullabot\\Parsely\\": "src/"
        }
