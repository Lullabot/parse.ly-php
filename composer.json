    "autoload-dev": {
        "psr-4": {
          "Lullabot\\Parsely\\Tests\\": "tests/src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^7.3",
        "symfony/console": "^4.1",
        "namshi/cuzzle": "^2.0",
        "rtheunissen/guzzle-log-middleware": "^0.4.1"
    }
}
